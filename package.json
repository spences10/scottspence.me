{
  "name": "scottspence.me",
  "description": "Personal portfolio with Gatsby",
  "version": "3.0.82",
  "author": "Scott Spence <yo@scottspence.dev>",
  "license": "MIT",
  "main": "n/a",
  "private": true,
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf public/",
    "build": "gatsby build && npm run wellknown",
    "serve": "gatsby serve -9000",
    "buildServe": "npm run build && npm run serve",
    "dev": "gatsby develop -p 6677 -o",
    "format": "pretty-quick",
    "release": "release-it",
    "wellknown": "cp -r .well-known/ public/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 70,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.6",
    "@mdx-js/react": "1.6.6",
    "@nivo/calendar": "0.62.0",
    "@nivo/pie": "0.62.0",
    "@wesbos/dump": "0.1.1",
    "babel-plugin-styled-components": "1.10.7",
    "date-fns": "2.14.0",
    "gatsby": "2.23.21",
    "gatsby-image": "2.4.12",
    "gatsby-plugin-catch-links": "2.3.10",
    "gatsby-plugin-fathom": "1.3.0",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "2.4.17",
    "gatsby-plugin-mdx": "1.2.22",
    "gatsby-plugin-offline": "3.2.16",
    "gatsby-plugin-react-helmet": "3.3.9",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sharp": "2.6.18",
    "gatsby-plugin-sitemap": "2.4.10",
    "gatsby-plugin-styled-components": "3.3.9",
    "gatsby-plugin-zeit-now": "0.3.0",
    "gatsby-remark-autolink-headers": "2.3.10",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-smartypants": "2.3.9",
    "gatsby-source-filesystem": "2.3.18",
    "gatsby-source-graphql": "2.6.1",
    "gatsby-transformer-sharp": "2.5.10",
    "react": "16.13.1",
    "react-device-detect": "1.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.10.0",
    "react-lottie": "1.2.3",
    "react-seo-component": "2.0.1",
    "styled-breakpoints": "8.1.1",
    "styled-components": "5.1.1",
    "styled-reset": "4.2.0"
  },
  "devDependencies": {
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1"
  },
  "keywords": [
    "gatsby",
    "portfolio"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/scottspence.me.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/scottspence.me/issues"
  },
  "homepage": "https://github.com/spences10/scottspence.me#readme"
}
