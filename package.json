{
  "name": "scottspence.me",
  "description": "Personal portfolio with Gatsby",
  "version": "1.0.5",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "gatsby": "1.9.277",
    "gatsby-image": "1.0.55",
    "gatsby-link": "1.6.46",
    "gatsby-plugin-favicon": "2.1.1",
    "gatsby-plugin-google-analytics": "1.0.31",
    "gatsby-plugin-manifest": "1.0.27",
    "gatsby-plugin-offline": "1.0.18",
    "gatsby-plugin-react-helmet": "2.0.11",
    "gatsby-plugin-react-next": "1.0.11",
    "gatsby-plugin-sharp": "1.6.48",
    "gatsby-plugin-styled-components": "2.0.11",
    "gatsby-source-filesystem": "1.5.39",
    "gatsby-transformer-remark": "1.7.44",
    "gatsby-transformer-sharp": "1.6.27",
    "react-helmet": "^5.2.0",
    "serve": "9.2.0",
    "styled-components": "3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "5.1.0",
    "eslint-plugin-graphql": "2.1.1",
    "eslint-plugin-react": "7.10.0",
    "husky": "0.14.3",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0"
  },
  "keywords": [
    "gatsby",
    "portfolio"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf scottportfolio/ && rm -rf public/",
    "now-start": "serve -s ./scottportfolio",
    "build": "gatsby build && mv public scottportfolio && npm run wellknown && npm run searchConsole",
    "dev": "npm run clean && gatsby develop",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "deploy": "now scottportfolio/ --token $NOW_TOKEN",
    "alias:dev": "now alias scottspence.now.sh --token $NOW_TOKEN",
    "alias:prod": "now alias scottspence.me --token $NOW_TOKEN",
    "cleanup": "now rm scottportfolio --safe --yes --token $NOW_TOKEN",
    "release": "release pre beta",
    "wellknown": "cp -r .well-known/ scottportfolio/",
    "searchConsole": "cp -r googleb08af286575fc468.html scottportfolio/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/scottspence.me.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/scottspence.me/issues"
  },
  "homepage": "https://github.com/spences10/scottspence.me#readme"
}
